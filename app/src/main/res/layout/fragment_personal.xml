                        <RelativeLayout
                            android:id="@+id/home_myselft_fragment_favorites"
                            android:background="@color/white"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/ts_48">


                            <TextView
                                android:layout_width="wrap_content"
                                android:text="我的收藏"
